&lt;p&gt;gitconfig&lt;/p&gt;
[user]
    name = zhixin
    email = wenzhixin2010@gmail.com
[alias]
    co = checkout
    ci = commit -a
    st = status
    br = branch
    oneline = log --pretty=oneline --since=&#39;2 days ago&#39;
    onelog = log -p -1
